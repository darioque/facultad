Object subclass: #Booking	instanceVariableNames: 'paquetes'	classVariableNames: ''	package: 'TP1_Carrizo90496_Olmos75539_Skidelsky75811_Poza87488'!!Booking methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!paquetes: anObject	paquetes := anObject! !!Booking methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!paquetes	^ paquetes! !!Booking methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/12/2022 16:52'!initialize 	paquetes := SortedCollection new.	paquetes sortBlock:[:a :b | a nombre < b nombre].! !!Booking methodsFor: 'propios' stamp: 'DarioSkidelsky 9/13/2022 12:49'!paqueteDentroDelPresupuesto: monto	^paquetes detect: [ :unPaquete | unPaquete calcularCosto <= monto ] ifNone: ['No existe ningun paquete dentro del presupuesto'] asString.! !!Booking methodsFor: 'propios' stamp: 'DarioSkidelsky 9/6/2022 19:01'!calcularMontoTotalEstimado 	| suma |	suma := 0.	paquetes do: [ :unPaquete | suma := suma + unPaquete calcularCosto].	^ suma.! !!Booking methodsFor: 'propios' stamp: 'DarioSkidelsky 9/12/2022 17:08'!existePaqueteConNombre: unNombre	| obj res |	res := false.	obj := paquetes detect: [ :unPaquete | unPaquete nombre = unNombre ] ifNone: [ nil ].	obj isNotNil ifTrue: [res := true  ].	^res.	! !!Booking methodsFor: 'propios' stamp: 'DarioSkidelsky 9/12/2022 17:27'!crucerosDisponibles: ciudadOrigen categoria: cat 	| paquetesViaCrucero crucerosDisponibles |		paquetesViaCrucero := paquetes select: [ :unPaquete | (unPaquete class = PaqueteViaCrucero ) & (unPaquete ciudadOrigen = ciudadOrigen)].		crucerosDisponibles := (paquetesViaCrucero select: [ :unPaquete | unPaquete existeCamaroteLibre: cat]).		^crucerosDisponibles asSortedCollection: [:unPaquete :otroPaquete | unPaquete nombreBarco > otroPaquete nombreBarco ].			! !!Booking methodsFor: 'converting' stamp: 'DarioSkidelsky 9/13/2022 13:24'!asString     | listadoPaquetes |    listadoPaquetes  := ''.    paquetes  do: [ :x | listadoPaquetes := listadoPaquetes, x nombre asString, String cr ].	 (paquetes isEmpty) ifTrue: [ listadoPaquetes  := '' ].	 	 ^ 'Listado de Paquetes: ', String cr, listadoPaquetes. ! !!Booking methodsFor: 'adding' stamp: 'DarioSkidelsky 9/12/2022 16:59'!addPaquete: unPaquete	| res |	res := false.	(unPaquete isKindOf: Paquete) ifTrue: [ (self existePaqueteConNombre: unPaquete nombre) ifFalse: [paquetes add: unPaquete. res := true.] ].		^res.! !Object subclass: #Camarote	instanceVariableNames: 'numero categoria precio reservado'	classVariableNames: ''	package: 'TP1_Carrizo90496_Olmos75539_Skidelsky75811_Poza87488'!!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!numero: anObject	numero := anObject! !!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/12/2022 16:15'!reservado	^ reservado.! !!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!precio: anObject	precio := anObject! !!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!reservado: anObject	reservado := anObject! !!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!precio	^ precio! !!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!numero	^ numero! !!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/1/2022 17:49'!categoria	^ categoria! !!Camarote methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/12/2022 16:12'!categoria: anObject	anObject isString ifTrue: [ ((anObject = 'suit') | (anObject = 'premium suit') | (anObject = 'turista')) 																				ifTrue: [categoria := anObject ] ].! !!Camarote methodsFor: 'propios' stamp: 'DarioSkidelsky 9/12/2022 16:17'!descripcionReservado	reservado ifTrue: [^'reservado'].	^ 'libre'.! !!Camarote methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/1/2022 19:41'!initialize	numero := 0.	categoria := ''.	precio := 0.	reservado := true.! !!Camarote methodsFor: 'converting' stamp: 'DarioSkidelsky 9/12/2022 16:22'!asString 	| str |		str := 'Numero de camarote: ', numero asString, String cr,			 'Categoria: ', categoria, String cr,			 'Precio por dia: $', precio asString, String cr,			 'Disponibilidad: ', self descripcionReservado, String cr.		^str.! !Object subclass: #Hotel	instanceVariableNames: 'nombre categoria disponibilidad precio'	classVariableNames: ''	package: 'TP1_Carrizo90496_Olmos75539_Skidelsky75811_Poza87488'!!Hotel methodsFor: 'propios' stamp: 'DarioSkidelsky 9/12/2022 16:30'!cantidadEstrellas	| cantidad |	cantidad := categoria size.		^cantidad.! !!Hotel methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/12/2022 16:28'!initialize 	categoria := ''.	disponibilidad := 0.	precio := 0.0.	nombre := ''.! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/12/2022 16:29'!precio: anObject	anObject isNumber ifTrue: [ precio := anObject ].! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:18'!precio	^ precio! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:18'!nombre: anObject	nombre := anObject! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:18'!disponibilidad	^ disponibilidad! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/12/2022 16:28'!disponibilidad: anObject	anObject isNumber ifTrue: [ disponibilidad := anObject ].! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:18'!categoria	^ categoria! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/12/2022 16:26'!categoria: anObject	((anObject isString) & (anObject size between: 1 and: 5)) ifTrue: [ categoria := anObject ].! !!Hotel methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:18'!nombre	^ nombre! !!Hotel methodsFor: 'converting' stamp: 'DarioSkidelsky 9/12/2022 16:25'!asString 	| str |		str := 'Nombre: ', nombre, String cr,	       'Categoria: ', categoria, ' Estrellas', String cr,			 'Cantidad de habitaciones disponibles: ', disponibilidad asString, String cr,			 'Precio por dia: $', precio asString, String cr.	^str.! !Object subclass: #Paquete	instanceVariableNames: 'nombre ciudadOrigen ciudadDestino estadia precioBase'	classVariableNames: ''	package: 'TP1_Carrizo90496_Olmos75539_Skidelsky75811_Poza87488'!!Paquete methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/12/2022 16:32'!initialize	nombre:= 'Paquete'.	ciudadOrigen := 'Cordoba'.	ciudadDestino := 'Buenos Aires'.	estadia := 0.	precioBase := 0.0.	! !!Paquete methodsFor: 'converting' stamp: 'DarioSkidelsky 9/12/2022 16:22'!asString     ^'Nombre: ', nombre asString, String cr, 'Ciudad Origen: ', ciudadOrigen asString, String cr , 'Ciudad Destino: ', ciudadDestino asString, String cr, 'Estadia: ', estadia asString, String cr, 'Precio Base: $', precioBase asString, String cr.! !!Paquete methodsFor: 'propios' stamp: 'DarioSkidelsky 9/6/2022 18:51'!calcularCosto	^self subclassResponsibility.! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!ciudadOrigen	^ ciudadOrigen! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!precioBase: anObject	precioBase := anObject! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!estadia	^ estadia! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!ciudadDestino: anObject	ciudadDestino := anObject! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!estadia: anObject	estadia := anObject! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!precioBase	^ precioBase! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!ciudadDestino	^ ciudadDestino! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!nombre: anObject	nombre := anObject! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!ciudadOrigen: anObject	ciudadOrigen := anObject! !!Paquete methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!nombre	^ nombre! !Paquete subclass: #PaqueteHotelero	instanceVariableNames: 'vuelo hotel'	classVariableNames: ''	package: 'TP1_Carrizo90496_Olmos75539_Skidelsky75811_Poza87488'!!PaqueteHotelero methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/1/2022 19:42'!initialize 	super initialize.	vuelo := Vuelo new.	hotel := Hotel new.! !!PaqueteHotelero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!hotel: anObject	hotel := anObject! !!PaqueteHotelero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!hotel	^ hotel! !!PaqueteHotelero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!vuelo	^ vuelo! !!PaqueteHotelero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!vuelo: anObject	vuelo := anObject! !!PaqueteHotelero methodsFor: 'converting' stamp: 'DarioSkidelsky 9/6/2022 19:41'!asString     ^ super asString, 'Vuelo: ', vuelo asString, String cr, 'Hotel: ', hotel asString, String cr.! !!PaqueteHotelero methodsFor: 'propios' stamp: 'DarioSkidelsky 9/12/2022 16:38'!calcularCosto		| precio cantEstrellas |		cantEstrellas := self hotel cantidadEstrellas.		precio := (precioBase * cantEstrellas) * estadia.				(cantEstrellas odd) ifTrue: [ ^precio * 0.80 ].		^precio.				! !Paquete subclass: #PaqueteViaCrucero	instanceVariableNames: 'nombreBarco camarotes'	classVariableNames: ''	package: 'TP1_Carrizo90496_Olmos75539_Skidelsky75811_Poza87488'!!PaqueteViaCrucero methodsFor: 'adding' stamp: 'DarioSkidelsky 9/1/2022 19:43'!addCamarote: unCamarote	camarotes add: unCamarote.! !!PaqueteViaCrucero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!nombreBarco	^ nombreBarco! !!PaqueteViaCrucero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!camarotes	^ camarotes! !!PaqueteViaCrucero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!nombreBarco: anObject	nombreBarco := anObject! !!PaqueteViaCrucero methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!camarotes: anObject	camarotes := anObject! !!PaqueteViaCrucero methodsFor: 'converting' stamp: 'DarioSkidelsky 9/12/2022 16:03'!asString    |datosCamarotes|    datosCamarotes := ''.    camarotes do: [ :x | datosCamarotes := datosCamarotes, x asString].	 (camarotes isEmpty) ifTrue: [ datosCamarotes := '' ].    ^super asString, 'Nombre del Barco: ', nombreBarco asString, String cr, 'Listado camarotes: ', String cr, datosCamarotes asString, String cr.! !!PaqueteViaCrucero methodsFor: 'propios' stamp: 'DarioSkidelsky 9/12/2022 17:26'!existeCamaroteLibre: cat	| obj res |	res := false.	obj := camarotes detect: [ :unCamarote | (unCamarote categoria = cat) & (unCamarote reservado not)]	ifNone: [ nil ].	obj isNotNil ifTrue: [ res := true ].	^res.! !!PaqueteViaCrucero methodsFor: 'propios' stamp: 'DarioSkidelsky 9/6/2022 19:43'!calcularCosto	| precioTurista |	precioTurista := (camarotes detect: [ :unCamarote | unCamarote categoria = 'turista' ]) precio.	^(precioBase + precioTurista) * estadia.! !!PaqueteViaCrucero methodsFor: 'propios' stamp: 'DarioSkidelsky 9/12/2022 16:49'!cantidadCamarotesDisponibles: categoria	| coleccion |	coleccion := camarotes select: [ :unCamarote | (unCamarote categoria = categoria) & (unCamarote reservado not) ].		^ coleccion size.	! !!PaqueteViaCrucero methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/1/2022 19:42'!initialize	super initialize.	camarotes := Bag new.	nombreBarco := ''.! !Object subclass: #Vuelo	instanceVariableNames: 'nombre tiempo'	classVariableNames: ''	package: 'TP1_Carrizo90496_Olmos75539_Skidelsky75811_Poza87488'!!Vuelo methodsFor: 'converting' stamp: 'DarioSkidelsky 9/12/2022 16:23'!asString 	| str |		str := 'Compa‚Äö√†√∂¬¨¬±ia: ', nombre, String cr, 			 'Duracion de vuelo: ', tiempo asString, 'hs', String cr.				^str.	! !!Vuelo methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/1/2022 19:41'!initialize	nombre := ''.	tiempo := 0.0.! !!Vuelo methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!nombre	^ nombre! !!Vuelo methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!nombre: anObject	nombre := anObject! !!Vuelo methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!tiempo: anObject	tiempo := anObject! !!Vuelo methodsFor: 'accessing' stamp: 'DarioSkidelsky 8/25/2022 20:19'!tiempo	^ tiempo! !