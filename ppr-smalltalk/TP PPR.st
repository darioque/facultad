ncorrectas.! !!Metrico methodsFor: 'validations' stamp: 'DarioSkidelsky 9/4/2021 18:25'!medidasCorrectas		|cantidadCorrectas|		cantidadCorrectas := 0.		medidas do: 		[:elem | elem = self valorMinimo 		ifTrue:[cantidadCorrectas := cantidadCorrectas + 1]]. 		^ cantidadCorrectas.! !Sensor1 subclass: #Termico	instanceVariableNames: 'valorMaximo'	classVariableNames: ''	package: 'TP PPR'!!Termico methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/4/2021 16:43'!initialize		super initialize.		valorMaximo := 0.! !!Termico methodsFor: 'validations' stamp: 'DarioSkidelsky 9/4/2021 18:24'!medidasIncorrectas		|cantidadIncorrectas|		cantidadIncorrectas := 0.		medidas do: 		[:elem | elem >= self valorMaximo		ifTrue:[cantidadIncorrectas := cantidadIncorrectas + 1]]. 		^ cantidadIncorrectas.! !!Termico methodsFor: 'validations' stamp: 'DarioSkidelsky 9/4/2021 18:25'!medidasCorrectas		|cantidadCorrectas|		cantidadCorrectas := 0.		medidas do: 		[:elem | elem < self valorMaximo		ifTrue:[cantidadCorrectas := cantidadCorrectas + 1]]. 		^ cantidadCorrectas.! !!Termico methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:36'!valorMaximo: anObject	valorMaximo := anObject! !!Termico methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:36'!valorMaximo	^ valorMaximo! !