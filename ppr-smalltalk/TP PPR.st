Object subclass: #Maquina	instanceVariableNames: 'sensores'	classVariableNames: ''	package: 'TP PPR'!!Maquina methodsFor: 'removing' stamp: 'DarioSkidelsky 9/4/2021 18:12'!quitarSensor: unSerial		|sensor|		sensor := sensores detect: [[:elem | elem serial = unSerial]] ifNone:[^false].		sensores remove: sensor.		^ sensores.! !!Maquina methodsFor: 'validations' stamp: 'DarioSkidelsky 9/4/2021 18:23'!medidasIncorrectas		|cantidadIncorrectas|		cantidadIncorrectas := 0.		sensores do: [:elem | cantidadIncorrectas = cantidadIncorrectas + elem medidasIncorrectas].		^ cantidadIncorrectas.! !!Maquina methodsFor: 'validations' stamp: 'DarioSkidelsky 9/4/2021 18:26'!estaEnBuenEstado		|cantidadMedidas|		cantidadMedidas := 0.		self sensores medidas do: [:elem | cantidadMedidas := cantidadMedidas + 1].		(self medidasCorrectas / cantidadMedidas) >= 0.80 ifTrue:[^ true].		^ false.! !!Maquina methodsFor: 'accumulator ops' stamp: 'DarioSkidelsky 9/4/2021 18:23'!medidasCorrectas		|cantidadCorrectas|		cantidadCorrectas := 0.		sensores do: [:elem | cantidadCorrectas = cantidadCorrectas + elem medidasCorrectas].		^ cantidadCorrectas.! !!Maquina methodsFor: 'accumulator ops' stamp: 'DarioSkidelsky 9/4/2021 18:40'!informarPorcentajeCorrectasHidra		|cantidadInc cantidadCor|		cantidadCor := 0.		cantidadInc := 0.		sensores do: 		[:elem | [elem isKindOf: Hidraulico] ifTrue:[cantidadCor := cantidadCor + elem medidasCorrectas ]].		sensores do: 		[:elem | [elem isKindOf: Hidraulico] ifTrue:[cantidadInc := cantidadInc + elem medidasIncorrectas ]].		^ (cantidadCor / cantidadInc) * 100.		! !!Maquina methodsFor: 'converting' stamp: 'DarioSkidelsky 9/4/2021 18:00'!asString		'Sensores: ', sensores asString.! !!Maquina methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:41'!sensores	^ sensores! !!Maquina methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 18:11'!existeSensor: unSerial		sensores detect: [[:elem | elem serial = unSerial]] ifNone:[^false].		^ true.! !!Maquina methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:41'!sensores: anObject	sensores := anObject! !!Maquina methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/4/2021 16:42'!initialize		sensores := OrderedCollection new.! !!Maquina methodsFor: 'adding' stamp: 'DarioSkidelsky 9/4/2021 17:46'!agregarSensor: unSensor		sensores detect: [:elem | elem serial = unSensor serial] ifNone:[sensores add: unSensor].		! !Object subclass: #Sensor1	instanceVariableNames: 'serial medidas'	classVariableNames: ''	package: 'TP PPR'!!Sensor1 methodsFor: 'validations' stamp: 'DarioSkidelsky 9/4/2021 18:06'!medidasIncorrectas		|cantidadIncorrectas|		cantidadIncorrectas := 0.		medidas do: [:elem | cantidadIncorrectas += elem medidasIncorrectas].		^ cantidadIncorrectas.! !!Sensor1 methodsFor: 'validations' stamp: 'DarioSkidelsky 9/4/2021 18:01'!medidasCorrectas		|cantidadCorrectas|		cantidadCorrectas := 0.		medidas do: [:elem | cantidadCorrectas += elem medidasCorrectas].		^ cantidadCorrectas.! !!Sensor1 methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:49'!serial	^ serial! !!Sensor1 methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:49'!serial: anObject	serial := anObject! !!Sensor1 methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:49'!medidas: anObject	medidas := anObject! !!Sensor1 methodsFor: 'accessing' stamp: 'DarioSkidelsky 9/4/2021 16:49'!medidas	^ medidas! !!Sensor1 methodsFor: 'initialization' stamp: 'DarioSkidelsky 9/4/2021 16:49'!initialize	"comment stating purpose of instance-side message"	"scope: class-variables  &  instance-variables"			medidas := OrderedCollection new.		serial := 0.! !!Sensor1 methodsFor: 'adding' stamp: 'DarioSkidelsky 9/4/2021 16:50'!agregarMedida: unaMedida		medidas add: unaMedida.! !Sensor1 subclass: #Hidraulico	instanceVariableNames: 'valorMinimo valorMaximo'	classVariableNames: ''	package: 'TP PPR'!!Hidr